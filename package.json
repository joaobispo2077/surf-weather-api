	"license": "ISC",
	"scripts": {
		"start": "node dist/src/index.js",
		"dev": "ts-node-dev --transpile-only src/index.ts | pino-pretty",
		"dev:win": "cmd.exe /c \"npx ts-node-dev --transpile-only .\\src\\index.ts | pino-pretty -C\"",
		"build": "tsc",
		"test": "yarn test:unit && yarn test:functional",
		"test:functional": "jest --projects ./test --runInBand",
		"test:unit": "jest",
		"debug:test:unit": "yarn test:unit --watch",
		"lint": "yarn lint:check && yarn style:check ",
		"lint:check": "eslint ./src ./test --ext .ts",
		"lint:fix": "eslint ./src ./test --ext .ts --fix",
		"style:check": "prettier --check ./src/**/*.ts ./test/**/*.ts",
		"style:fix": "prettier --write src/**/*.ts test/**/*.ts"
	},
	"dependencies": {
		"@overnightjs/core": "^1.7.6",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"config": "^3.3.6",
		"cors": "^2.8.5",
		"dayjs": "^1.10.6",
		"express": "^4.17.1",
		"express-openapi-validator": "^4.12.14",
		"express-pino-logger": "^6.0.0",
		"express-rate-limit": "^5.3.0",
		"http-status-codes": "^2.1.4",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.2",
		"mongoose": "^5.13.2",
		"pino": "^6.12.0",
		"swagger-ui-express": "^4.1.6"
	},
	"devDependencies": {
