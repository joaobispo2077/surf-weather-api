		"eslint": "^7.29.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"jest": "^27.0.4",
		"nock": "^13.1.1",
		"pino-pretty": "^5.1.1",
		"prettier": "^2.3.1",
